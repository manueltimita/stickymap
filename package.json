{
  "name": "stickymap",
  "version": "1.3.3",
  "main": "src/index.js",
  "scripts": {
    "pretest": "eslint src test",
    "test": "karma start test/config.js --single-run",
    "test-forever": "karma start test/config.js",
    "serve": "browser-sync start --server build --files build --reload-debounce 1000 --no-open",
    "rebuild": "nodemon --ext js,html --watch src --watch doc --exec make build",
    "start": "npm run rebuild & npm run serve"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/tschaub/stickymap.git"
  },
  "bugs": {
    "url": "https://github.com/tschaub/stickymap/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-tschaub": "^13.1.0",
    "karma": "^4.4.1",
    "karma-browserify": "^7.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-source-map-support": "^1.4.0",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "pixelmatch": "^5.1.0",
    "watchify": "^3.11.1"
  },
  "eslintConfig": {
    "extends": "tschaub"
  }
}
